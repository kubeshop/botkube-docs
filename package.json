{
  "name": "docusaurus-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "tsc": "tsc",
    "lint": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint:check": "eslint . --ext .json,.js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint . --fix --ext .json,.js,.jsx,.ts,.tsx",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:cli-docs-fix": "prettier --write ./docs/cli/commands",
    "qa": "npm run tsc && npm run lint && npm run build"
  },
  "dependencies": {
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/plugin-client-redirects": "^3.5.2",
    "@docusaurus/plugin-content-docs": "^3.5.2",
    "@docusaurus/plugin-google-gtag": "^3.5.2",
    "@docusaurus/plugin-google-tag-manager": "^3.5.2",
    "@docusaurus/plugin-ideal-image": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.1.1",
    "docusaurus-plugin-sass": "^0.2.5",
    "node-fetch": "^3.3.2",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-cookie-banner": "^4.1.0",
    "react-dom": "^18.3.1",
    "react-player": "^2.16.0",
    "sass": "^1.78.0"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^3.5.2",
    "@docusaurus/module-type-aliases": "^3.5.2",
    "@docusaurus/types": "^3.5.2",
    "@tsconfig/docusaurus": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.3",
    "typescript": "~5.5.0",
    "image-size": "~1.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": ">=20 <21"
  },
  "volta": {
    "node": "20.10.0"
  }
}
