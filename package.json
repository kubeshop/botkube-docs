{
  "name": "docusaurus-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "tsc": "tsc",
    "lint": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint:check": "eslint . --ext .json,.js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint . --fix --ext .json,.js,.jsx,.ts,.tsx",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:cli-docs-fix": "prettier --write ./docs/cli/commands",
    "qa": "npm run tsc && npm run lint && npm run build"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/plugin-client-redirects": "^2.4.1",
    "@docusaurus/plugin-google-gtag": "^2.4.1",
    "@docusaurus/plugin-google-tag-manager": "^2.4.1",
    "@docusaurus/plugin-ideal-image": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "docusaurus-plugin-sass": "^0.2.5",
    "node-fetch": "^2.6.9",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-cookie-banner": "^4.1.0",
    "react-dom": "^17.0.2",
    "sass": "^1.58.3"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^2.4.1",
    "@docusaurus/module-type-aliases": "^2.4.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16.16.0 <17.0.0"
  }
}
